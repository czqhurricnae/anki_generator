{
  "apps": [
    {
      "name": "anki_generator",
      "script": "/var/lib/jenkins/workspace/anki_generator/server.js",
      "cwd": "/var/lib/jenkins/workspace/anki_generator",
      "instances": 1,
      "exec_mode": "cluster_mode",
      "max_memory_restart": "200M",
      "max_restarts": 40,
      "watch": false,
      "error_file": "./logs/pm2.error.log",
      "out_file": "./logs/pm2.log",
      "env": {
        "NODE_ENV": "producton"
      }
    }
  ]
}
